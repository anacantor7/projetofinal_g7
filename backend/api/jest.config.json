{
  "testEnvironment": "node",
  "testMatch": [
    "**/__tests__/**/*.js",
    "**/?(*.)+(spec|test).js"
  ],
  "collectCoverageFrom": [
    "**/*.js",
    "!**/node_modules/**",
    "!**/coverage/**",
    "!app.js",
    "!database/db.js"
  ],
  "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"]
}
